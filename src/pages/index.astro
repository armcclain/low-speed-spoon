---
import Layout from '../layouts/BaseLayout.astro';
import ImageFrame from '../components/ImageFrame.astro';
---
<Layout title="the low-speed spoon">
  <section class="mx-auto max-w-6xl px-4 py-24">
    <div class="grid place-items-center">
      <div class="w-full max-w-sm">
        <!-- Replace the src with your final logo path -->
        <img src="/assets/Low_Speed_Spoon.png" alt="the low-speed spoon logo" class="w-full h-auto" />
      </div>
      <div class="mt-10 text-xs lowercase tracking-widest text-gray-500">
        <!-- intentionally minimal, no tagline per your preference -->
      </div>
    </div>
  </section>
</Layout>
